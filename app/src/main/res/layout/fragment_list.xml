<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/t"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#B4B4B4"
    android:orientation="vertical">


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#34495E">

            <TextView
                android:id="@+id/listName"
                android:layout_width="81dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="25dp"
                android:text="BMW"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:translationX="100dp" />

            <TextView
                android:layout_width="113dp"
                android:layout_height="45dp"
                android:layout_marginStart="180dp"
                android:layout_marginTop="25dp"
                android:layout_toEndOf="@id/listName"
                android:text="Precio: 250.000.000"
                android:textColor="@android:color/white"
                android:textSize="16sp" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#34495E"
                android:src="@drawable/bmw_svgrepo_com" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#34495E">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_marginStart="30dp"
                android:layout_marginTop="25dp"
                android:text="Mercedes"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:translationX="100dp" />

            <TextView
                android:layout_width="113dp"
                android:layout_height="45dp"
                android:layout_marginStart="180dp"
                android:layout_marginTop="25dp"
                android:text="Precio: 300.000.000"
                android:textColor="@android:color/white"
                android:textSize="16sp"
                android:translationX="120dp" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#34495E"
                />

            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#34495E"
                android:src="@drawable/mercedes_benz_svgrepo_com"
                android:translationX="5dp" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#34495E">

            <TextView
                android:layout_width="81dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="25dp"
                android:text="Audi"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:translationX="100dp" />

            <TextView
                android:layout_width="113dp"
                android:layout_height="45dp"
                android:layout_marginStart="180dp"
                android:layout_marginTop="25dp"
                android:text="Precio: 280.000.000"
                android:textColor="@android:color/white"
                android:textSize="16sp"
                android:translationX="120dp" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#34495E"
                android:src="@drawable/audi_svgrepo_com"
                android:translationX="5dp" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#34495E">

            <TextView
                android:layout_width="81dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="25dp"
                android:text="Tesla"
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:translationX="100dp" />

            <TextView
                android:layout_width="113dp"
                android:layout_height="45dp"
                android:layout_marginStart="180dp"
                android:layout_marginTop="25dp"
                android:text="Precio: 350.000.000"
                android:textColor="@android:color/white"
                android:textSize="16sp"
                android:translationX="120dp" />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#34495E"
               />

            <ImageButton
                android:id="@+id/imageButton12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#34495E"
                android:src="@drawable/brand_tesla_svgrepo_com" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>
